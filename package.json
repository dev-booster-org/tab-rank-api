{
  "name": "api-tab-rank",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "tsx --watch src/server.ts",
    "start": "node dist/server.js",
    "typeorm": "typeorm-ts-node-esm -d ./src/database/typeorm-datasource.ts",
    "migration:create": "typeorm-ts-node-esm migration:create src/database/migrations/$npm_config_name",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:generate": "typeorm-ts-node-esm migration:generate src/database/migrations/$npm_config_name -d ./src/database/typeorm-datasource.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "tsyringe": "^4.10.0",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.0",
    "eslint": "^8.56.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
